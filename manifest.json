{
    "name": "fb-ads-finder",
    "version": "0.1",
    "manifest_version" : 2,
    "description": "Does some simple stuff",
    "background" : {
      "scripts" : ["background.js"]
    },
    "browser_action": {
      "default_icon": "logo.png"
    },
    "permissions": ["activeTab"],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["DoNothing.js"],
          "css": ["styles.css"]
        }
      ]
}
